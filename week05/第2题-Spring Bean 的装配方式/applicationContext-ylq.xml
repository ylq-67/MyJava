<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                        http://www.springframework.org/schema/context
                        http://www.springframework.org/schema/context/spring-context-3.2.xsd http://www.springframework.org/schema/aop https://www.springframework.org/schema/aop/spring-aop.xsd">
    


    <bean id="xmlbean" class="io.ylq.springbean.XmlBean"> </bean>

    <context:component-scan base-package="io.ylq.springbean" />

    <bean id="autoWireBean" class="io.ylq.springbean.AutoWireBean"></bean>
    <bean id="beanUser" class="io.ylq.springbean.BeanUser"></bean>
</beans>